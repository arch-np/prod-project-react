    "react-dom": "^17.0.2",
    "react-i18next": "^14.0.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.2"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
